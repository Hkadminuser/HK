<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="njhk.wisdom.web.service.mapper.relation.WxReturnvisitVisitinfoDao">

    <sql id="projectColumns">
        a.visitinfo_id AS "visitinfo_id",
        a.returnvisit_id AS "returnvisit_id"
    </sql>

    <sql id="projectJoins">
    </sql>

    <select id="get" resultType="njhk.wisdom.web.bean.entity.relevance.WxReturnvisitVisitinfo">
        SELECT
        <include refid="projectColumns"/>
        FROM wxPersonServepgd a
        <include refid="projectJoins"/>
        <where>
            1=1
        </where>
    </select>

    <select id="findList" resultType="njhk.wisdom.web.bean.entity.relevance.WxReturnvisitVisitinfo">
        SELECT
        <include refid="projectColumns"/>
        FROM wxPersonServepgd a
        <include refid="projectJoins"/>
        <where>
            1=1
        </where>
        <choose>
            <when test="page !=null and page.orderBy != null and page.orderBy != ''">
                ORDER BY ${page.orderBy}
            </when>
        </choose>
    </select>




    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO wxPersonServepgd(
            visitinfo_id,
            returnvisit_id
        ) VALUES (
                     #{visitinfo_id},
                     #{returnvisit_id}
                 )
    </insert>

    <update id="update">
        UPDATE wxPersonServepgd SET
                                    visitinfo_id=#{visitinfo_id},
                                    returnvisit_id=#{returnvisit_id}
        WHERE ID = #{id}
    </update>

    <update id="delete">
        UPDATE wxServetypeServestation SET
            del_flag = #{DEL_FLAG_DELETE}
        WHERE visitinfo_id=#{visitinfo_id}
    </update>

</mapper>
